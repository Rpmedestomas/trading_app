<%= form_for @user, url: edit_admin_path do |f| %>
    <div class="field">
     <%= f.label :email %><br />
     <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
   </div>
 
   <div class="field">
     <%= f.label :full_name %><br />
     <%= f.text_field :full_name %>
   </div>
 
   <div class="actions">
     <%= f.submit 'Edit' %>
     <%= link_to 'Cancel', admin_index_path %>
   </div>
 <% end %>